<div class="modal fade" id="updateLeadModal">
  <div class="modal-dialog modal-xl">
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title">
          <%= singleTitle %> Update #
          <span class="leadIdUpdate">000</span>
        </h4>
        <button
          type="button"
          class="close"
          data-dismiss="modal"
          aria-label="Close"
        >
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body"><%- include('./partial_update_form'); %></div>
    </div>
  </div>
</div>
